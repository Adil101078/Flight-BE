    <script>
        $(document).ready(function () {
            
            // $('#form1').on('submit', function (e) {
            //     if (e.isDefaultPrevented()) {
            //         // handle the invalid form...
            //     } else {
                    
            //         $('.popupModel').show();

            //     }
            // })
            });
            
            
            $('#GetData').on('click', (e)=>{
                e.preventDefault()
                $('.popupModel').show();
                const firstName = $("input[name='firstName[]']").map(function(){return $(this).val();}).get();
                const middleName = $("input[name='middleName[]']").map(function(){return $(this).val();}).get();
                const lastName = $("input[name='lastName[]']").map(function(){return $(this).val();}).get();
                const dobDay = $("select[name='dobDay[]']").map(function(){return $(this).val();}).get();
                const dobMonth = $("select[name='dobMonth[]']").map(function(){return $(this).val();}).get();
                const dobYear = $("select[name='dobYear[]']").map(function(){return $(this).val();}).get();
                const title = $("select[name='title[]']").map(function(){return $(this).val();}).get();
                const gender = $("select[name='gender[]']").map(function(){return $(this).val();}).get();
                const type = $("input[name='paxType[]']").map(function(){return $(this).val();}).get();

                const arrayOfObjects = firstName.map((currentValue, index) => {
                    return{
                        sl_no: currentValue,
                        firstName: firstName[index],
                        lastName: lastName[index],
                        middleName: middleName[index],
                        day: dobDay[index],
                        month: dobMonth[index],
                        year: dobYear[index],
                        title: title[index],
                        gender: gender[index],
                        type: type[index]
                    }
                    })
                const currency = $('input[name="currency"]').val()
                const totalPrice = `<%=data.response.totalPrice%>`
                const jType = `<%=data.jtype%>`
                const origin = $('input[name="origin"]').val()
                const destination = $('input[name="destination"]').val()
                const fareFamily = $('input[name="fareFamily"]').val()
                const valCarrier = $('input[name="valCarrier"]').val()
                const email = $('input[name="email"]').val()
                const mobile = $('input[name="mobile_no"]').val()
                const cardHolder = $('input[name="cardHolder"]').val()
                const cardNumber = $('input[name="cardNumber"]').val()
                const cardExpMonth = $('select[name="cardExpMonth"]').val()
                const cardExpYear = $('select[name="cardExpYear"]').val()
                const cardCvv = $('input[name="cvv"]').val()
                const address1 = $('input[name="Address1"]').val()
                const address2 = $('input[name="Address2"]').val()
                const country = $('select[name="country"]').val()
                const region = $('input[name="Region"]').val()
                const city = $('input[name="City"]').val()
                const postal = $('input[name="PostCode"]').val()
                const lastTicketingDate = `<%=data.response.lastTicketingDate%>`
                const data = {
                    currency,
                    totalPrice,
                    jType,
                    origin,
                    destination,
                    fareFamily,
                    valCarrier,
                    email,
                    mobile,
                    address1,
                    address2,
                    region,
                    city,
                    postal,
                    country,
                    passengers: arrayOfObjects,
                    cardHolder,
                    cardNumber,
                    cardCvv,
                    cardExpMonth,
                    cardExpYear,
                    lastTicketingDate

                }
               $.ajax({
                url:`flight/create-booking`,
                type:'POST',
                data: JSON.stringify(data),
                contentType: 'application/json',
                success:(res)=>{
                    window.location = `${window.origin}/flight/booking-confirmation?bookingId=${res.data.Booking_Id}`
                },
                error:(err)=>{
                    alert(err.responseJSON.message)
                }
               })

            }) 
    </script>


