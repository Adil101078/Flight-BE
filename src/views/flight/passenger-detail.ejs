                                    const arr = sector.filter((e)=> e.group === group)
                                    let result
                                    if(!isFirst){
                                        result = data._.last(arr)
                                        
                                    } else{
                                        result = arr[0]
                                    }
                                
                                    return result
                                }

                                function findSector({ sector, group }){
                                    return sector.filter((e)=> e.group == group)
                                    
                                }
                             
